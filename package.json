{
  "private": true,
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "dev": "concurrently \"npm --prefix decoracoesapi run dev\" \"npm --prefix deCoracoesfront run dev\"",
    "submodule-update-api": "cd decoracoesapi && git submodule update --init --recursive",
    "submodule-update-front": "cd deCoracoesfront && git submodule update --init --recursive",
    "submodule-update": "npm-run-all submodule-update-api submodule-update-front"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
